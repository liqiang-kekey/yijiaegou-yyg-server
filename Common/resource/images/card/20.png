yForm, flag)\n          } else {\n            this.$message.error(this.$t('tips.fillValidate'))\n          }\n        })\n      } else {\n        this.handleAjaxForm(queryForm, flag)\n      }\n    },\n\n    /**\n     * @method 处理表单回显和提交\n     */\n    handleAjaxForm (formInfo, flag) {\n      this.$api.editOrGetCardInfo(formInfo).then(res => {\n        if (res.code === 200) {\n          if (flag !== 1) {\n            let {data} = res\n            data.vr_cover = data.vr_cover ? [{url: data.vr_cover}] : ''\n            this.cardFrom = {...data, action: ''}\n          } else {\n            this.$message.success(this.$t('tips.successSub'))\n          }\n        }\n      })\n    },\n\n    /**\n     * @method 重置表单\n     */\n    resetForm () {\n      this.$refs['cardFrom'].resetFields()\n    },\n    /**\n     * @method 获取封面图\n     */\n    getVRCover (img) {\n      this.cardFrom.vr_cover = img\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n    .lb-card-setting{\n        width: 100%;\n        .page-main{\n          .card-form{\n            .el-input{\n              width: 300px;\n            }\n          }\n        }\n    }\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/view/businessCard/set/cardSetting.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"lb-card-setting\"},[_c('top-nav'),_vm._v(\" \"),_c('div',{staticClass:\"page-main\"},[_c('el-form',{ref:\"cardFrom\",staticClass:\"card-form\",attrs:{\"model\":_vm.cardFrom,\"rules\":_vm.cardFromRules,\"label-width\":\"140px\"},nativeOn:{\"submit\":function($event){$event.preventDefault();}}},[_c('el-form-item',{attrs:{\"label\":\"分配方式\",\"prop\":\"auto_switch_way\"}},[_c('el-radio-group',{model:{value:(_vm.cardFrom.auto_switch_way),callback:function ($$v) {_vm.$set(_vm.cardFrom, \"auto_switch_way\", $$v)},expression:\"cardFrom.auto_switch_way\"}},[_c('el-radio',{attrs:{\"label\":1}},[_vm._v(\"随机\")]),_vm._v(\" \"),_c('el-radio',{attrs:{\"lab